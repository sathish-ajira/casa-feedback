<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tech.ajira.casa_feedback">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/casa_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.RetailerLogin"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.CustomerLogin"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.Dashboard"
            android:label="Dashboard"
            android:theme="@style/NoActionBar" />
        <activity android:name=".activities.CustomerFeedback" />
        <activity android:name=".activities.QuestionEditActivity"></activity>
    </application>

</manifest>